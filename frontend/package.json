    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@rushstack/eslint-patch": "^1.6.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "playwright": "^1.56.1",
